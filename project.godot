[display]

window/stretch/mode="viewport"

[editor]

movie_writer/movie_file="/Users/quentintopper/alt_ctrl.avi"
movie_writer/fps=30

[editor_plugins]

enabled=PackedStringArray("res://addons/aseprite_importer/plugin.cfg")

